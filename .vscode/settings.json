{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true
    },
    "terminal.integrated.env.linux": {
        "PATH": "/home/vagrant/miniconda3/envs/myProject/bin:${env:PATH}"
    },
    "python.condaPath": "/home/vagrant/miniconda3/condabin/conda",
    "python.venvPath": "/home/vagrant/miniconda3/envs",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.activateEnvironment": true,
    "python.venvFolders": [
        "/home/vagrant/miniconda3/envs/myProject"
    ],
    "python.pythonPath": "/home/vagrant/miniconda3/envs/myProject/bin/python",
    "python.linting.pylintPath": "/home/vagrant/miniconda3/envs/myProject/bin/pylint",
    "python.linting.enabled": true,
    "python.testing.nosetestsEnabled": false,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "."
    ],
    "python.jediEnabled": false
}